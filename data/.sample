~some comment
let a: { b: "something non-color", c: palette.num };

let palette: {
    blue: [#007bff, #0056b3, #004b9b, a.b],
    gray: [#f8f9fa, #e9ecef, #dee2e6, #ced4da],
    dw: [0...10^10],
    white: [#9aed7b...#4154d1^5:FLAT], ~has 5 shades and uses LCH interpolation
    num: 10,
    selfA: palette.selfB,
    selfB: #000000
};

theme light: {
    primary: palette.blue[0],
    secondary: #00ff00,
    background: palette.white[0],
    text: palette.gray[3],
    button: {
        active: palette.blue[2],
        hover: palette.blue[1]
    }
};

theme dark: {
    primary: palette.blue[1],
    secondary: palette.blue[0],
    background: #000000,
    text: #ffffff,
    button: {
        active: palette.blue[2],
        hover: palette.blue[1]
    }
};

font montserrat: {
    weights: [1...9], ~automatically has 9 weights
    style: "all"
} from 'url';